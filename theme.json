{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "black",
					"color": "#0B0B0B",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#FFFFFF",
					"name": "White"
				},
				{
					"slug": "grey-med",
					"color": "#D8D8D8",
					"name": "Grey medium"
				},
				{
					"slug": "grey-light",
					"color": "#F2F2F2",
					"name": "Grey light"
				},
				{
					"slug": "grey",
					"color": "#323740",
					"name": "Grey"
				},
				{
					"slug": "grey-dark",
					"color": "#18212F",
					"name": "Grey dark"
				},
				{
					"slug": "green",
					"color": "#90CA5B",
					"name": "Green"
				},
				{
					"slug": "green-dark",
					"color": "#007B52",
					"name": "Green dark"
				},
				{
					"slug": "blue-light",
					"color": "#0095CB",
					"name": "Blue light"
				},
				{
					"slug": "blue-dark",
					"color": "#006095",
					"name": "blue-Dark"
				},
				{
					"slug": "pink",
					"color": "##FF4A60",
					"name": "Pink"
				},
				{
					"slug": "red",
					"color": "#ED0C2A",
					"name": "Red"
				},
				{
					"slug": "burgundy",
					"color": "#B10937",
					"name": "Burgundy"
				},
				{
					"slug": "orange-red",
					"color": "#CC110D",
					"name": "Orange red"
				},
				{
					"slug": "orange",
					"color": "#FFB500",
					"name": "Orange"
				},
				{
					"slug": "yellow",
					"color": "#FFD76B",
					"name": "Yellow"
				},
				{
					"slug": "aquamarine-dark",
					"color": "#007684",
					"name": "Aquamarine dark"
				},
				{
					"slug": "aquamarine",
					"color": "#00C0B7",
					"name": "Aquamarine"
				},
				{
					"slug": "blue-baby",
					"color": "#EAF2FD",
					"name": "Blue baby"
				},
				{
					"slug": "red-light",
					"color": "#FFEDEF",
					"name": "Red liight"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "primary-white",
					"gradient": "linear-gradient(#0073aa 49.9%,#fff 50%)",
					"name": "Primary color to white"
				},
				{
					"slug": "white-primary",
					"gradient": "linear-gradient(#fff 49.9%,#0073aa 50%)",
					"name": "Horizontal white to Primary color"
				}
			],
			"duotone": [
				{
					"colors": [
						"#00a0d2",
						"#0073aa"
					],
					"slug": "secondary-primary",
					"name": "Secondary and primary"
				},
				{
					"colors": [
						"#0073aa",
						"#f0f0f0"
					],
					"slug": "primary-contrast",
					"name": "Primary and Contrast"
				}
			]
		},
		"layout": {
			"contentSize": "1160px",
			"wideSize": "1280px"
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "\"Times New Roman\",\"New York\",Times,\"Noto Serif\",serif"
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "12px",
					"name": "Small",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "14px",
					"name": "Medium",
					"fluid": false
				},
				{
					"slug": "large",
					"size": "16px",
					"name": "Large",
					"fluid": {
						"min": "1.75rem",
						"max": "2rem"
					}
				},
				{
					"slug": "x-large",
					"size": "18px",
					"name": "XL",
					"fluid": {
						"min": "2.5rem",
						"max": "2.75rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "24px",
					"name": "XXL",
					"fluid": {
						"min": "3rem",
						"max": "3.75rem"
					}
				},
				{
					"slug": "h1",
					"size": "48px",
					"name": "H1",
					"fluid": {}
				},
				{
					"slug": "h2",
					"size": "38px",
					"name": "H2",
					"fluid": {}
				},
				{
					"slug": "h3",
					"size": "30px",
					"name": "H3",
					"fluid": {}
				},
				{
					"slug": "h4",
					"size": "24px",
					"name": "H4",
					"fluid": {}
				},
				{
					"slug": "h5",
					"size": "18px",
					"name": "H5",
					"fluid": {}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"css": "& .wp-block-site-title{ margin:0; font-weight:400;}",
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--60)",
						"bottom": "var(--wp--preset--spacing--60)"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/post-template": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/query-pagination": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/search": {
				"css": ".wp-block-search__button-inside .wp-block-search__inside-wrapper{border: none;}",
				"typography": {
					"lineHeight": "1"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--13)",
						"right": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--13)",
						"left": "var(--wp--preset--spacing--20)"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "2px",
					"style": "solid",
					"radius": "10px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			}
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--system)",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": [
				"post",
				"page"
			]
		}
	]
}
